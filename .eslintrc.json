{
    "extends": "eslint-config-fluid",
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": 2019
    },
    "rules": {
        "strict": ["error", "global"],
        "no-loop-func": 0,
        "no-console": 0
    },
    "ignorePatterns": ["dist", "!.eslintrc.js", "!.eleventy.js", "src/_data"]
}
